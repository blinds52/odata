        },
        "Org.OData.Capabilities.V1.DeleteRestrictionsType": {
            "type": "object",
            "properties": {
                "Deletable": {
                    "type": [
                        "boolean",
                        "null"
                    ],
                    "default": true,
                    "description": "Entities can be deleted"
                },
                "NonDeletableNavigationProperties": {
                    "type": "array",
                    "items": {
                        "$ref": "http://docs.oasis-open.org/odata/odata-json-csdl/v4.0/edm.json#/definitions/Edm.NavigationPropertyPath"
                    },
                    "description": "These navigation properties do not allow DeleteLink requests"
                }
            }
        }
    },
    "schemas": {
        "Org.OData.Core.V1": {
            "$ref": "https://tools.oasis-open.org/version-control/browse/wsvn/odata/trunk/spec/vocabularies/Org.OData.Core.V1.json#/schemas/Org.OData.Core.V1"
        },
        "Core": {
            "$ref": "https://tools.oasis-open.org/version-control/browse/wsvn/odata/trunk/spec/vocabularies/Org.OData.Core.V1.json#/schemas/Org.OData.Core.V1"
        },
        "Capabilities": {
            "$ref": "#/schemas/Org.OData.Capabilities.V1"
        },
        "Org.OData.Capabilities.V1": {
            "description": "\n          The Capabilities vocabulary aims to provide a way for service authors to describe\n          certain capabilities of an OData Service.\n        ",
            "@Core.LongDescription": "\n          There are some capabilities which are strongly recommended for services to support even\n          though they are optional. Support for $top and $skip is a good example as\n          supporting these query options helps with performance of a service and are essential. Such\n          capabilities are assumed to be default capabilities of an OData service even in\n          the case that a capabilities annotation doesn’t exist. Capabilities annotations are\n          mainly expected to be used to explicitly specify that a service doesn’t support such\n          capabilities. Capabilities annotations can as well be used to declaratively\n          specify the support of such capabilities.\n\n          On the other hand, there are some capabilities that a service may choose to support or\n          not support and in varying degrees. $filter and $orderby are such good examples.\n          This vocabulary aims to define terms to specify support or no support for such\n          capabilities.\n\n          A service is assumed to support by default the following capabilities even though an\n          annotation doesn’t exist:\n          - Countability ($count)\n          - Client pageability ($top, $skip)\n          - Expandability ($expand)\n          - Indexability by key\n          - Batch support ($batch)\n          - Navigability of navigation properties\n\n          A service is expected to support the following capabilities. If not supported, the\n          service is expected to call out the restrictions using annotations:\n          - Filterability ($filter)\n          - Sortability ($orderby)\n          - Queryability of top level entity sets\n          - Query functions\n\n          A client cannot assume that a service supports certain capabilities. A client can try, but\n          it needs to be prepared to handle an error in case the following capabilities are not\n          supported:\n          - Insertability\n          - Updatability\n          - Deletability\n        ",
            "terms": {
                "ConformanceLevel": {
                    "anyOf": [
                        {
                            "$ref": "#/definitions/Org.OData.Capabilities.V1.ConformanceLevelType"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "appliesTo": "EntityContainer"
                },
                "SupportedFormats": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    },
                    "appliesTo": "EntityContainer",
                    "description": "Media types of supported formats, including format parameters",
                    "@Core.IsMediaType": {

                    }
                },
                "SupportedMetadataFormats": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    },
                    "appliesTo": "EntityContainer",
                    "description": "Media types of supported formats for $metadata, including format parameters",
                    "@Core.IsMediaType": {

                    }
                },
                "AcceptableEncodings": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    },
                    "appliesTo": "EntityContainer",
                    "description": "List of acceptable compression methods for ($batch) requests, e.g. gzip"
                },
                "AsynchronousRequestsSupported": {
                    "anyOf": [
                        {
                            "$ref": "https://tools.oasis-open.org/version-control/browse/wsvn/odata/trunk/spec/vocabularies/Org.OData.Core.V1.json#/definitions/Org.OData.Core.V1.Tag"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": true,
                    "appliesTo": "EntityContainer",
                    "description": "Service supports the asynchronous request preference"
                },
                "BatchContinueOnErrorSupported": {
                    "anyOf": [
                        {
                            "$ref": "https://tools.oasis-open.org/version-control/browse/wsvn/odata/trunk/spec/vocabularies/Org.OData.Core.V1.json#/definitions/Org.OData.Core.V1.Tag"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": true,
                    "appliesTo": "EntityContainer",
                    "description": "Service supports the continue on error preference"
                },
                "IsolationSupported": {
                    "anyOf": [
                        {
                            "$ref": "#/definitions/Org.OData.Capabilities.V1.IsolationLevel"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "appliesTo": "EntityContainer",
                    "description": "Supported odata.isolation levels"
                },
                "CallbackSupported": {
                    "anyOf": [
                        {
                            "$ref": "#/definitions/Org.OData.Capabilities.V1.CallbackType"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "appliesTo": [
                        "EntityContainer",
                        "EntitySet"
                    ],
                    "description": "Supports callbacks for the specified protocols"
                },
                "CrossJoinSupported": {
                    "anyOf": [
                        {
                            "$ref": "https://tools.oasis-open.org/version-control/browse/wsvn/odata/trunk/spec/vocabularies/Org.OData.Core.V1.json#/definitions/Org.OData.Core.V1.Tag"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": true,
                    "appliesTo": "EntityContainer",
                    "description": "Supports cross joins for the entity sets in this container"
                },
                "ChangeTracking": {
                    "anyOf": [
                        {
                            "$ref": "#/definitions/Org.OData.Capabilities.V1.ChangeTrackingType"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "appliesTo": [
                        "EntityContainer",
                        "EntitySet"
                    ],
                    "description": "Change tracking capabilities of this service or entity set"
                },
                "CountRestrictions": {
                    "anyOf": [
                        {
                            "$ref": "#/definitions/Org.OData.Capabilities.V1.CountRestrictionsType"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "appliesTo": "EntitySet",
                    "description": "Restrictions on /$count path suffix and $count=true system query option"
                },
                "NavigationRestrictions": {
                    "anyOf": [
                        {
                            "$ref": "#/definitions/Org.OData.Capabilities.V1.NavigationRestrictionsType"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "appliesTo": "EntitySet",
                    "description": "Restrictions on navigating properties according to OData URL conventions"
                },
                "IndexableByKey": {
                    "anyOf": [
                        {
                            "$ref": "https://tools.oasis-open.org/version-control/browse/wsvn/odata/trunk/spec/vocabularies/Org.OData.Core.V1.json#/definitions/Org.OData.Core.V1.Tag"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": true,
                    "appliesTo": "EntitySet",
                    "description": "Supports key values according to OData URL conventions"
                },
                "TopSupported": {
                    "anyOf": [
                        {
                            "$ref": "https://tools.oasis-open.org/version-control/browse/wsvn/odata/trunk/spec/vocabularies/Org.OData.Core.V1.json#/definitions/Org.OData.Core.V1.Tag"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": true,
                    "appliesTo": "EntitySet",
                    "description": "Supports $top"
                },
                "SkipSupported": {
                    "anyOf": [
                        {
                            "$ref": "https://tools.oasis-open.org/version-control/browse/wsvn/odata/trunk/spec/vocabularies/Org.OData.Core.V1.json#/definitions/Org.OData.Core.V1.Tag"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": true,
                    "appliesTo": "EntitySet",
                    "description": "Supports $skip"
                },
                "BatchSupported": {
                    "anyOf": [
                        {
                            "$ref": "https://tools.oasis-open.org/version-control/browse/wsvn/odata/trunk/spec/vocabularies/Org.OData.Core.V1.json#/definitions/Org.OData.Core.V1.Tag"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": true,
                    "appliesTo": "EntityContainer",
                    "description": "Supports $batch requests"
                },
                "BatchContentIdReferencingInRequestBodiesSupported": {
                    "anyOf": [
                        {
                            "$ref": "https://tools.oasis-open.org/version-control/browse/wsvn/odata/trunk/spec/vocabularies/Org.OData.Core.V1.json#/definitions/Org.OData.Core.V1.Tag"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": true,
                    "appliesTo": "EntityContainer",
                    "description": "Service supports Content-ID referencing in request bodies"
                },
                "FilterFunctions": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    },
                    "appliesTo": [
                        "EntityContainer",
                        "EntitySet"
                    ],
                    "description": "List of functions supported in $filter"
                },
                "FilterRestrictions": {
                    "anyOf": [
                        {
                            "$ref": "#/definitions/Org.OData.Capabilities.V1.FilterRestrictionsType"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "appliesTo": "EntitySet",
                    "description": "Restrictions on $filter expressions"
                },
                "SortRestrictions": {
                    "anyOf": [
                        {
                            "$ref": "#/definitions/Org.OData.Capabilities.V1.SortRestrictionsType"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "appliesTo": "EntitySet",
                    "description": "Restrictions on $orderby expressions"
                },
                "ExpandRestrictions": {
                    "anyOf": [
                        {
                            "$ref": "#/definitions/Org.OData.Capabilities.V1.ExpandRestrictionsType"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "appliesTo": "EntitySet",
                    "description": "Restrictions on $expand expressions"
                },
                "SearchRestrictions": {
                    "anyOf": [
                        {
                            "$ref": "#/definitions/Org.OData.Capabilities.V1.SearchRestrictionsType"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "appliesTo": "EntitySet",
                    "description": "Restrictions on $search expressions"
                },
                "InsertRestrictions": {
                    "anyOf": [
                        {
                            "$ref": "#/definitions/Org.OData.Capabilities.V1.InsertRestrictionsType"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "appliesTo": "EntitySet",
                    "description": "Restrictions on insert operations"
                },
                "UpdateRestrictions": {
                    "anyOf": [
                        {
                            "$ref": "#/definitions/Org.OData.Capabilities.V1.UpdateRestrictionsType"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "appliesTo": "EntitySet",
                    "description": "Restrictions on update operations"
                },
                "DeleteRestrictions": {
                    "anyOf": [
                        {
                            "$ref": "#/definitions/Org.OData.Capabilities.V1.DeleteRestrictionsType"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "appliesTo": "EntitySet",
                    "description": "Restrictions on delete operations"
                }
            }
        }
    }
}
