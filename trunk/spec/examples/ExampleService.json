          "type":"boolean"
        },
        "ShippingAddress":{
          "odataType":"Model.Address"
        }
      },
      "navigationProperties":{
        "Items":{
          "type":"Collection(Model.OrderItem)",
          "partner":"Order"
        },
        "Customer":{
          "type":"Model.Customer",
          "nullable":false,
          "partner":"Orders"
        }
      }
    },
    "ODataDemo.VipCustomer":{
