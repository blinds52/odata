          "odataType":"Collection(Edm.String)"
        },
        "Rating":{
          "type":"integer",
          "minimum":-2147483648,
          "maximum":2147483647
        },
        "ReleaseDate":{
          "odataType":"Edm.Date"
        },
        "DiscontinuedDate":{
          "odataType":"Edm.Date"
        },
