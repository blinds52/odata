          "minimum":-2147483648,
          "maximum":2147483647
        },
        "ItemID":{
          "type":"string"
        },
        "Size":{
          "type":"string"
        },
        "Price":{
          "type":"number"
        }
      },
      "navigationProperties":{
        "Order":{
          "type":"Model.Order",
          "nullable":false,
          "partner":"Items",
          "onDelete":{
            "action":"Cascade"
          },
          "referentialConstraints":{
            "OrderID":{
              "referencedProperty":"ID"
            }
          }
        },
        "Product":{
          "type":"Model.Product",
          "nullable":false,
          "partner":"OrderItems"
        },
        "Currency":{
          "type":"Model.Currency",
          "nullable":false
        }
      }
    },
    "ODataDemo.Sales":{
      "$ref":"#/definitions/Model.Sales"
    },
    "Model.Sales":{
      "typekind":"EntityType",
      "keys":{
        "ID":"ID"
      },
      "properties":{
        "ID":{
          "type":"integer",
          "minimum":-2147483648,
          "maximum":2147483647
        },
        "Amount":{
          "type":"number"
        }
      },
      "navigationProperties":{
        "Time":{
          "type":"Model.Time",
          "nullable":false,
          "partner":"Sales"
        },
        "Currency":{
          "type":"Model.Currency",
          "nullable":false,
          "partner":"Sales"
        },
        "Customer":{
          "type":"Model.Customer",
          "nullable":false,
          "partner":"Sales"
        },
        "SalesOrganization":{
          "type":"Model.SalesOrganization",
          "nullable":false,
          "partner":"Sales"
        },
        "Product":{
          "type":"Model.Product",
          "nullable":false,
          "partner":"Sales"
        }
      }
    },
    "ODataDemo.Employee":{
      "$ref":"#/definitions/Model.Employee"
    },
    "Model.Employee":{
      "typekind":"EntityType",
      "hasStream":true,
      "keys":{
        "ID":"ID"
      },
      "properties":{
        "ID":{
          "type":"string"
        },
        "FirstName":{
          "type":"string"
        },
        "LastName":{
          "type":"string"
        },
        "Title":{
          "type":"string"
        }
      },
      "navigationProperties":{
        "Manager":{
          "type":"Model.Manager",
          "partner":"Employees"
        },
        "LeaveRequests":{
          "type":"Collection(Model.LeaveRequest)",
          "partner":"Employee"
        }
      }
    },
    "ODataDemo.LeaveRequest":{
      "$ref":"#/definitions/Model.LeaveRequest"
    },
    "Model.LeaveRequest":{
      "typekind":"EntityType",
      "keys":{
        "ID":"ID"
      },
      "properties":{
        "ID":{
          "type":"integer",
          "minimum":-2147483648,
          "maximum":2147483647
        }
      },
      "navigationProperties":{
        "Manager":{
          "type":"Model.Manager",
          "nullable":false,
          "partner":"Inbox"
        },
        "Employee":{
          "type":"Model.Employee",
          "nullable":false,
          "partner":"LeaveRequests"
        }
      }
    },
    "ODataDemo.Address":{
      "$ref":"#/definitions/Model.Address"
    },
    "Model.Address":{
      "typekind":"ComplexType",
      "properties":{
        "Street":{
          "type":"string"
        },
        "Region":{
          "type":"string"
        },
        "PostalCode":{
          "type":"string"
        }
      },
      "navigationProperties":{
        "Country":{
          "type":"Example.Country",
          "nullable":false
        },
        "City":{
          "type":"Example.City",
          "nullable":false
        }
      }
    },
    "ODataDemo.AddressWithLocation":{
      "$ref":"#/definitions/Model.AddressWithLocation"
    },
    "Model.AddressWithLocation":{
      "typekind":"ComplexType",
      "properties":{
        "Location":{
          "odataType":"Edm.GeographyPoint"
        }
      }
    },
    "ODataDemo.Country":{
      "$ref":"#/definitions/Model.Country"
    },
    "Model.Country":{
      "typekind":"EntityType",
      "keys":{
        "Code":"Code"
      },
      "properties":{
        "Code":{
          "type":"string",
          "maxLength":2
        },
        "Name":{
          "type":"string"
        },
        "Continent":{
          "type":"string"
        }
      }
    },
    "ODataDemo.PreferredSupplier":{
      "$ref":"#/definitions/Model.PreferredSupplier"
    },
    "Model.PreferredSupplier":{
      "typekind":"EntityType",
      "baseType":"Model.Supplier",
      "properties":{
        "EmployeeID":{
          "type":"string"
        }
      },
      "navigationProperties":{
        "AccountRepresentative":{
          "type":"Model.Employee",
          "nullable":false,
          "referentialConstraints":{
            "EmployeeID":{
              "referencedProperty":"ID",
              "@Core.Description":{
                "string":"The key account manager"
              }
            }
          }
        }
      }
    },
    "ODataDemo.Manager":{
      "$ref":"#/definitions/Model.Manager"
    },
    "Model.Manager":{
      "typekind":"EntityType",
      "baseType":"Model.Employee",
      "navigationProperties":{
        "Employees":{
          "type":"Collection(Model.Employee)",
          "partner":"Manager"
        },
        "Inbox":{
          "type":"Collection(Model.LeaveRequest)",
          "partner":"Manager"
        }
      }
    },
    "ODataDemo.Time":{
      "$ref":"#/definitions/Model.Time"
    },
    "Model.Time":{
      "typekind":"EntityType",
      "keys":{
        "Date":"Date"
      },
      "properties":{
        "Date":{
          "odataType":"Edm.Date"
        },
        "Year":{
          "type":"string",
          "maxLength":4
        },
        "Quarter":{
          "type":"string"
        },
        "Month":{
          "type":"string"
        }
      },
      "navigationProperties":{
        "Sales":{
          "type":"Collection(Model.Sales)",
          "partner":"Time"
        }
      }
    },
    "ODataDemo.SalesOrganization":{
      "$ref":"#/definitions/Model.SalesOrganization"
    },
    "Model.SalesOrganization":{
      "typekind":"EntityType",
      "keys":{
        "ID":"ID"
      },
      "properties":{
        "ID":{
          "type":"string"
        },
        "Name":{
          "type":"string"
        }
      },
      "navigationProperties":{
        "Sales":{
          "type":"Collection(Model.Sales)",
          "partner":"SalesOrganization"
        },
        "Superordinate":{
          "type":"Model.SalesOrganization"
        }
      }
    },
    "ODataDemo.Currency":{
      "$ref":"#/definitions/Model.Currency"
    },
    "Model.Currency":{
      "typekind":"EntityType",
      "keys":{
        "Code":"Code"
      },
      "properties":{
        "Code":{
          "type":"string",
          "maxLength":3
        },
        "Name":{
          "type":"string"
        },
        "Symbol":{
          "type":"string"
        },
        "FractionalDigits":{
          "type":"integer",
          "minimum":0,
          "maximum":255
        }
      },
      "navigationProperties":{
        "Sales":{
          "type":"Collection(Model.Sales)",
          "partner":"Currency"
        }
      }
    },
    "ODataDemo.City":{
