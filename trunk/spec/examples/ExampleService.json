{
  "$schema":"http://odata.org/schemas/v4.0/edm.json#",
  "definitions":{
    "OData.Demo.Customer":{
      "type":"object",
      "odata":{
        "kind":"entity",
        "keys":{
          "ID":"ID"
        }
      },
      "properties":{
        "ID":{
          "type":"string"
        },
        "Orders":{
          "type":"array",
          "items":{
            "$ref":"#/definitions/Model.Order"
          },
          "odata":{
            "kind":"navigation",
            "partner":"Customer"
          }
        },
        "Name":{
          "type":"string"
        },
        "CompanyName":{
          "type":[
            "string",
            "null"
          ]
        },
        "BirthDate":{
          "type":"string"
        },
        "CountryName":{
          "type":"string",
          "@Core.Description":"This is weird, given that we have a Country entity type"
        },
        "Sales":{
          "type":"array",
          "items":{
            "$ref":"#/definitions/Model.Sales"
          },
          "odata":{
            "kind":"navigation",
            "partner":"Customer"
          }
        },
        "ContactName":{
          "type":"string"
        },
        "ContactTitle":{
          "type":"string"
        },
        "Fax":{
          "type":"string"
        },
        "Address":{
          "$ref":"#/definitions/Model.Address"
        },
        "EmailAddresses":{
          "type":"array",
          "items":{
            "type":"string"
          }
        },
        "Phone":{
          "$ref":"#/definitions/Model.PhoneNumber"
        }
      }
    },
    "OData.Demo.Supplier":{
      "type":"object",
      "odata":{
        "kind":"entity",
        "keys":{
          "ID":"ID"
        }
      },
      "properties":{
        "ID":{
          "type":"string"
        },
        "Products":{
          "type":"array",
          "items":{
            "$ref":"#/definitions/Model.Product"
          },
          "odata":{
            "kind":"navigation",
            "partner":"Supplier"
          }
        },
        "Name":{
          "type":"string"
        },
        "EmailAddresses":{
          "type":"array",
          "items":{
            "type":"string"
          }
        },
        "Addresses":{
          "type":"array",
          "items":{
            "$ref":"#/definitions/Model.Address"
          }
        }
      }
    },
    "OData.Demo.Product":{
      "type":"object",
      "odata":{
        "kind":"entity",
        "keys":{
          "ID":"ID"
        }
      },
      "properties":{
        "ID":{
          "$ref":"http://odata.org/schemas/v4.0/edm.json#/definitions/Edm.Int32"
        },
        "Category":{
          "$ref":"#/definitions/Model.Category",
          "odata":{
            "kind":"navigation",
            "partner":"Products"
          }
        },
        "Name":{
          "type":"string"
        },
        "Supplier":{
          "$ref":"#/definitions/Model.Supplier",
          "odata":{
            "kind":"navigation",
            "partner":"Products"
          }
        },
        "Description":{
          "type":"string"
        },
        "Thumbnail":{
          "type":"string"
        },
        "Sizes":{
          "type":"array",
          "items":{
            "type":"string"
          }
        },
        "Rating":{
          "$ref":"http://odata.org/schemas/v4.0/edm.json#/definitions/Edm.Int32"
        },
        "OrderItems":{
          "type":"array",
          "items":{
            "$ref":"#/definitions/Model.OrderItem"
          },
          "odata":{
            "kind":"navigation",
            "partner":"Product"
          }
        },
        "ReleaseDate":{
          "$ref":"http://odata.org/schemas/v4.0/edm.json#/definitions/Edm.Date"
        },
        "DiscontinuedDate":{
          "anyOf":[
            {
              "$ref":"http://odata.org/schemas/v4.0/edm.json#/definitions/Edm.Date"
            },
            {
              "type":"null"
            }
          ]
        },
        "LifeTime":{
          "type":"string"
        },
        "Color":{
          "type":"string"
        },
        "TaxRate":{
          "type":[
            "number",
            "null"
          ],
          "multipleOf":1e-6,
          "odata":{
            "precision":"10"
          }
        },
        "Sales":{
          "type":"array",
          "items":{
            "$ref":"#/definitions/Model.Sales"
          },
          "odata":{
            "kind":"navigation",
            "partner":"Product"
          }
        }
      }
    },
    "OData.Demo.Order":{
      "type":"object",
      "odata":{
        "kind":"entity",
        "keys":{
          "ID":"ID"
        }
      },
      "properties":{
        "ID":{
          "$ref":"http://odata.org/schemas/v4.0/edm.json#/definitions/Edm.Int32"
        },
        "Items":{
          "type":"array",
          "items":{
            "$ref":"#/definitions/Model.OrderItem"
          },
          "odata":{
            "kind":"navigation",
            "partner":"Order"
          }
        },
        "Completed":{
          "type":"boolean"
        },
        "Customer":{
          "$ref":"#/definitions/Model.Customer",
          "odata":{
            "kind":"navigation",
            "partner":"Orders"
          }
        },
        "ShippingAddress":{
          "$ref":"#/definitions/Model.Address"
        }
      }
    },
    "OData.Demo.VipCustomer":{
      "allOf":[
        {
          "$ref":"http://odata.org/schemas/v4.0/edm.json#/definitions/Model.Customer"
        },
        {
          "type":"object",
          "odata":{
            "kind":"entity"
          }
        }
      ]
    },
    "OData.Demo.Category":{
      "type":"object",
      "odata":{
        "kind":"entity",
        "keys":{
          "ID":"ID"
        }
      },
      "properties":{
        "ID":{
          "$ref":"http://odata.org/schemas/v4.0/edm.json#/definitions/Edm.Int32"
        },
        "Amount":{
          "type":"number"
        },
        "Products":{
          "type":"array",
          "items":{
            "$ref":"#/definitions/Model.Product"
          },
          "odata":{
            "kind":"navigation",
            "partner":"Category"
          }
        },
        "Name":{
          "type":"string"
        },
        "Thumbnail":{
          "type":"string"
        }
      }
    },
    "OData.Demo.OrderItem":{
      "type":"object",
      "odata":{
        "kind":"entity",
        "keys":{
          "OrderID":"OrderID",
          "ItemID":"ItemID"
        }
      },
      "properties":{
        "OrderID":{
          "$ref":"http://odata.org/schemas/v4.0/edm.json#/definitions/Edm.Int32"
        },
        "ItemID":{
          "type":"string"
        },
        "Order":{
          "$ref":"#/definitions/Model.Order",
          "odata":{
            "kind":"navigation",
            "partner":"Items",
            "onDelete":{
              "action":"Cascade"
            },
            "referentialConstraints":{
              "OrderID":{
                "referencedProperty":"ID"
              }
            }
          }
        },
        "Size":{
          "type":"string"
        },
        "Price":{
          "type":"number"
        },
        "Product":{
          "$ref":"#/definitions/Model.Product",
          "odata":{
            "kind":"navigation",
            "partner":"OrderItems"
          }
        },
        "Currency":{
          "$ref":"#/definitions/Model.Currency",
          "odata":{
            "kind":"navigation"
          }
        }
      }
    },
    "OData.Demo.Sales":{
      "type":"object",
      "odata":{
        "kind":"entity",
        "keys":{
          "ID":"ID"
        }
      },
      "properties":{
        "ID":{
          "$ref":"http://odata.org/schemas/v4.0/edm.json#/definitions/Edm.Int32"
        },
        "Amount":{
          "type":"number"
        },
        "Time":{
          "$ref":"#/definitions/Model.Time",
          "odata":{
            "kind":"navigation",
            "partner":"Sales"
          }
        },
        "Currency":{
          "$ref":"#/definitions/Model.Currency",
          "odata":{
            "kind":"navigation",
            "partner":"Sales"
          }
        },
        "Customer":{
          "$ref":"#/definitions/Model.Customer",
          "odata":{
            "kind":"navigation",
            "partner":"Sales"
          }
        },
        "SalesOrganization":{
          "$ref":"#/definitions/Model.SalesOrganization",
          "odata":{
            "kind":"navigation",
            "partner":"Sales"
          }
        },
        "Product":{
          "$ref":"#/definitions/Model.Product",
          "odata":{
            "kind":"navigation",
            "partner":"Sales"
          }
        }
      }
    },
    "OData.Demo.Employee":{
      "type":"object",
      "odata":{
        "kind":"entity",
        "hasStream":true,
        "keys":{
          "ID":"ID"
        }
      },
      "properties":{
        "ID":{
          "type":"string"
        },
        "FirstName":{
          "type":"string"
        },
        "LastName":{
          "type":"string"
        },
        "Title":{
          "type":"string"
        },
        "Manager":{
          "anyOf":[
            {
              "$ref":"#/definitions/Model.Manager"
            },
            {
              "type":"null"
            }
          ],
          "odata":{
            "kind":"navigation",
            "partner":"Employees"
          }
        },
        "LeaveRequests":{
          "type":"array",
          "items":{
            "$ref":"#/definitions/Model.LeaveRequest"
          },
          "odata":{
            "kind":"navigation",
            "partner":"Employee"
          }
        }
      }
    },
    "OData.Demo.LeaveRequest":{
      "type":"object",
      "odata":{
        "kind":"entity",
        "keys":{
          "ID":"ID"
        }
      },
      "properties":{
        "ID":{
          "$ref":"http://odata.org/schemas/v4.0/edm.json#/definitions/Edm.Int32"
        },
        "Manager":{
          "$ref":"#/definitions/Model.Manager",
          "odata":{
            "kind":"navigation",
            "partner":"Inbox"
          }
        },
        "Employee":{
          "$ref":"#/definitions/Model.Employee",
          "odata":{
            "kind":"navigation",
            "partner":"LeaveRequests"
          }
        }
      }
    },
    "OData.Demo.Address":{
      "type":"object",
      "odata":{
        "kind":"complex"
      },
      "properties":{
        "Street":{
          "type":"string"
        },
        "Country":{
          "$ref":"#/definitions/Example.Country",
          "odata":{
            "kind":"navigation"
          }
        },
        "City":{
          "$ref":"#/definitions/Example.City",
          "odata":{
            "kind":"navigation"
          }
        },
        "Region":{
          "type":"string"
        },
        "PostalCode":{
          "type":"string"
        }
      }
    },
    "OData.Demo.AddressWithLocation":{
      "type":"object",
      "odata":{
        "kind":"complex"
      },
      "properties":{
        "Location":{
          "$ref":"http://odata.org/schemas/v4.0/edm.json#/definitions/Edm.GeographyPoint"
        }
      }
    },
    "OData.Demo.Country":{
      "type":"object",
      "odata":{
        "kind":"entity",
        "keys":{
          "Code":"Code"
        }
      },
      "properties":{
        "Code":{
          "type":"string",
          "maxLength":2
        },
        "Name":{
          "type":"string"
        },
        "Continent":{
          "type":"string"
        }
      }
    },
    "OData.Demo.PreferredSupplier":{
      "allOf":[
        {
          "$ref":"http://odata.org/schemas/v4.0/edm.json#/definitions/Model.Supplier"
        },
        {
          "type":"object",
          "odata":{
            "kind":"entity"
          },
          "properties":{
            "AccountRepresentative":{
              "anyOf":[
                {
                  "$ref":"#/definitions/Model.Employee"
                },
                {
                  "type":"null"
                }
              ],
              "odata":{
                "kind":"navigation",
                "referentialConstraints":{
                  "EmployeeID":{
                    "referencedProperty":"ID",
                    "@Core.Description":"The key account manager"
                  }
                }
              }
            },
            "EmployeeID":{
              "type":"string"
            }
          }
        }
      ]
    },
    "OData.Demo.Pattern":{
      "enum":[
        "Yellow",
        1,
        "Solid",
        2
      ],
      "odata":{
        "isFlags":true
      }
    },
    "OData.Demo.Manager":{
      "allOf":[
        {
          "$ref":"http://odata.org/schemas/v4.0/edm.json#/definitions/Model.Employee"
        },
        {
          "type":"object",
          "odata":{
            "kind":"entity"
          },
          "properties":{
            "Employees":{
              "type":"array",
              "items":{
                "$ref":"#/definitions/Model.Employee"
              },
              "odata":{
                "kind":"navigation",
                "partner":"Manager"
              }
            },
            "Inbox":{
              "type":"array",
              "items":{
                "$ref":"#/definitions/Model.LeaveRequest"
              },
              "odata":{
                "kind":"navigation",
                "partner":"Manager"
              }
            }
          }
        }
      ]
    },
    "OData.Demo.Time":{
      "type":"object",
      "odata":{
        "kind":"entity",
        "keys":{
          "Date":"Date"
        }
      },
      "properties":{
        "Date":{
          "$ref":"http://odata.org/schemas/v4.0/edm.json#/definitions/Edm.Date"
        },
        "Year":{
          "type":"string",
          "maxLength":4
        },
        "Quarter":{
          "type":"string"
        },
        "Month":{
          "type":"string"
        },
        "Sales":{
          "type":"array",
          "items":{
            "$ref":"#/definitions/Model.Sales"
          },
          "odata":{
            "kind":"navigation",
            "partner":"Time"
          }
        }
      }
    },
    "OData.Demo.SalesOrganization":{
      "type":"object",
      "odata":{
        "kind":"entity",
        "keys":{
          "ID":"ID"
        }
      },
      "properties":{
        "ID":{
          "type":"string"
        },
        "Name":{
          "type":"string"
        },
        "Sales":{
          "type":"array",
          "items":{
            "$ref":"#/definitions/Model.Sales"
          },
          "odata":{
            "kind":"navigation",
            "partner":"SalesOrganization"
          }
        },
        "Superordinate":{
          "anyOf":[
            {
              "$ref":"#/definitions/Model.SalesOrganization"
            },
            {
              "type":"null"
            }
          ],
          "odata":{
            "kind":"navigation"
          }
        }
      }
    },
    "OData.Demo.Currency":{
      "type":"object",
      "odata":{
        "kind":"entity",
        "keys":{
          "Code":"Code"
        }
      },
      "properties":{
        "Code":{
          "type":"string",
          "maxLength":3
        },
        "Name":{
          "type":"string"
        },
        "Sales":{
          "type":"array",
          "items":{
            "$ref":"#/definitions/Model.Sales"
          },
          "odata":{
            "kind":"navigation",
            "partner":"Currency"
          }
        },
        "Symbol":{
          "type":"string"
        },
        "FractionalDigits":{
          "$ref":"http://odata.org/schemas/v4.0/edm.json#/definitions/Edm.Byte"
        }
      }
    },
    "OData.Demo.City":{
      "type":"object",
      "odata":{
        "kind":"entity",
        "keys":{
          "ID":"ID"
        }
      },
      "properties":{
        "ID":{
          "$ref":"http://odata.org/schemas/v4.0/edm.json#/definitions/Edm.Guid"
        },
        "Name":{
          "type":"string"
        }
      }
    },
    "OData.Demo.PhoneNumber":{
      "type":"object",
      "odata":{
        "kind":"complex"
      },
      "properties":{
        "Number":{
          "type":"string"
        },
        "Type":{
          "type":"string"
        }
      }
    },
    "OData.Demo.CellPhoneNumber":{
      "type":"object",
      "odata":{
        "kind":"complex"
      },
      "properties":{
        "Carrier":{
          "type":"string"
        }
      }
    }
  },
  "schemas":{
    "OData.Demo":{
      "alias":"Model",
      "entityContainer":{
        "name":"Container",
        "entitySets":{
          "Customers":{
            "entityType":"Model.Customer",
            "navigationPropertyBindings":{
              "Orders":{
                "target":"Orders"
              },
              "Sales":{
                "target":"Sales"
              },
              "Address/Country":{
                "target":"Countries"
              }
            }
          },
          "Suppliers":{
            "entityType":"Model.Supplier",
            "navigationPropertyBindings":{
              "Products":{
                "target":"Products"
              },
              "Model.PreferredSupplier/AccountRepresentative":{
                "target":"Employees"
              }
            }
          },
          "Products":{
            "entityType":"Model.Product",
            "navigationPropertyBindings":{
              "Category":{
                "target":"Categories"
              },
              "Supplier":{
                "target":"Suppliers"
              },
              "OrderItems":{
                "target":"OrderItems"
              },
              "Sales":{
                "target":"Sales"
              }
            }
          },
          "Orders":{
            "entityType":"Model.Order",
            "navigationPropertyBindings":{
              "Items":{
                "target":"OrderItems"
              },
              "Customer":{
                "target":"Customers"
              }
            }
          },
          "Categories":{
            "entityType":"Model.Category",
            "navigationPropertyBindings":{
              "Products":{
                "target":"Products"
              }
            }
          },
          "OrderItems":{
            "entityType":"Model.OrderItem",
            "navigationPropertyBindings":{
              "Order":{
                "target":"Orders"
              },
              "Product":{
                "target":"Products"
              },
              "Currency":{
                "target":"Currencies"
              }
            }
          },
          "Sales":{
            "entityType":"Model.Sales",
            "navigationPropertyBindings":{
              "Time":{
                "target":"Time"
              },
              "Currency":{
                "target":"Currencies"
              },
              "Customer":{
                "target":"Customers"
              },
              "SalesOrganization":{
                "target":"SalesOrganizations"
              },
              "Product":{
                "target":"Products"
              }
            }
          },
          "Employees":{
            "entityType":"Model.Employee",
            "navigationPropertyBindings":{
              "Model.Manager/Employees":{
                "target":"Employees"
              },
              "Manager":{
                "target":"Employees"
              },
              "Model.Manager/Inbox":{
                "target":"LeaveRequests"
              },
              "LeaveRequests":{
                "target":"LeaveRequests"
              }
            }
          },
          "LeaveRequests":{
            "entityType":"Model.LeaveRequest",
            "navigationPropertyBindings":{
              "Manager":{
                "target":"Employees"
              },
              "Employee":{
                "target":"Employees"
              }
            }
          },
          "Countries":{
            "entityType":"Model.Country"
          },
          "Time":{
            "entityType":"Model.Time",
            "navigationPropertyBindings":{
              "Sales":{
                "target":"Sales"
              }
            }
          },
          "SalesOrganizations":{
            "entityType":"Model.SalesOrganization",
            "navigationPropertyBindings":{
              "Sales":{
                "target":"Sales"
              },
              "Superordinate":{
                "target":"SalesOrganizations"
              }
            }
          },
          "Currencies":{
            "entityType":"Model.Currency",
            "navigationPropertyBindings":{
              "Sales":{
                "target":"Sales"
              }
            }
          },
          "Cities":{
            "entityType":"Model.City"
          }
        }
      }
    }
  },
  "references":{
    "http://tinyurl.com/Org-OData-Core":{
      "includes":{
        "Org.OData.Core.V1":{
          "alias":"Core"
        }
      }
    }
  }
}