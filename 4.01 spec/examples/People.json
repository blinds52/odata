{
    "$Version": "4.0",
    "$EntityContainer": "PeopleService.Container",
    "$Reference": {
        "http://localhost/examples/Products.xml": {
            "$Include": [
                {
                    "$Namespace": "ProductService",
                    "$Alias": "R0I0"
                }
            ]
        },
        "/dev/null": {
            "$Include": [
                {
                    "$Namespace": "Org.OData.Display.V1",
                    "$Alias": "Display"
                },
                {
                    "$Namespace": "Org.OData.Publication.V1",
                    "$Alias": "Publication"
                }
            ]
        }
    },
    "PeopleService": {
        "$Kind": "Schema",
        "$Alias": "self",
        "$Annotations": {
            "self.Container": {
                "@Display.Description": "This is a sample OData service with vocabularies"
            },
            "self.Product": {
                "@Display.Description": "All Products available in the online store"
            },
            "self.Product/Name": {
                "@Display.DisplayName": "Product Name"
            },
            "self.Container/Suppliers": {
                "@Publication.PublisherName": "Microsoft Corp.",
                "@Publication.PublisherId": "MSFT",
                "@Publication.Keywords": "Inventory, Supplier, Advertisers, Sales, Finance",
                "@Publication.AttributionUrl": "http://www.odata.org/",
                "@Publication.AttributionDescription": "All rights reserved",
                "@Publication.DocumentationUrl ": "http://www.odata.org/",
                "@Publication.TermsOfUseUrl": "All rights reserved",
                "@Publication.PrivacyPolicyUrl": "http://www.odata.org/",
                "@Publication.LastModified": "4/2/2013",
                "@Publication.ImageUrl ": "http://www.odata.org/"
            }
        },
        "Supplier": {
            "$Kind": "EntityType",
            "$Key": [
                "ID"
            ],
            "ID": {
                "$Type": "Edm.Int32",
                "$Nullable": false
            },
            "Name": {

            },
            "Address": {
                "$Type": "self.Address"
            },
            "Location": {
                "$Type": "Edm.GeographyPoint",
                "$SRID": "variable"
            },
            "Concurrency": {
                "$Type": "Edm.Int32",
                "$Nullable": false
            },
            "Products": {
                "$Kind": "NavigationProperty",
                "$Collection": true,
                "$Type": "R0I0.Product",
                "$Partner": "Supplier"
            }
        },
        "Address": {
            "$Kind": "ComplexType",
            "Street": {

            },
            "City": {

            },
            "State": {

            },
            "ZipCode": {

            },
            "Country": {

            }
        },
        "Person": {
            "$Kind": "EntityType",
            "$Key": [
                "ID"
            ],
            "ID": {
                "$Type": "Edm.Int32",
                "$Nullable": false
            },
            "Name": {

            },
            "PersonDetail": {
                "$Kind": "NavigationProperty",
                "$Type": "self.PersonDetail",
                "$Partner": "Person"
            }
        },
        "Customer": {
            "$Kind": "EntityType",
            "$BaseType": "self.Person",
            "TotalExpense": {
                "$Type": "Edm.Decimal",
                "$Nullable": false
            }
        },
        "Employee": {
            "$Kind": "EntityType",
            "$BaseType": "self.Person",
            "EmployeeID": {
                "$Type": "Edm.Int64",
                "$Nullable": false
            },
            "HireDate": {
                "$Type": "Edm.DateTimeOffset",
                "$Nullable": false
            },
            "Salary": {
                "$Type": "Edm.Single",
                "$Nullable": false
            }
        },
        "PersonDetail": {
            "$Kind": "EntityType",
            "$Key": [
                "PersonID"
            ],
            "PersonID": {
                "$Type": "Edm.Int32",
                "$Nullable": false
            },
            "Age": {
                "$Type": "Edm.Byte",
                "$Nullable": false
            },
            "Gender": {
                "$Type": "Edm.Boolean",
                "$Nullable": false
            },
            "Phone": {

            },
            "Address": {
                "$Type": "self.Address"
            },
            "Photo": {
                "$Type": "Edm.Stream",
                "$Nullable": false
            },
            "Person": {
                "$Kind": "NavigationProperty",
                "$Type": "self.Person",
                "$Partner": "PersonDetail"
            }
        },
        "Container": {
            "$Kind": "EntityContainer",
            "Suppliers": {
                "$Kind": "EntitySet",
                "$Type": "self.Supplier",
                "$NavigationPropertyBinding": {
                    "Products": "R0I0.Container/Products"
                }
            },
            "People": {
                "$Kind": "EntitySet",
                "$Type": "self.Person",
                "$NavigationPropertyBinding": {
                    "PersonDetail": "PersonDetails"
                }
            },
            "PersonDetails": {
                "$Kind": "EntitySet",
                "$Type": "self.PersonDetail",
                "$NavigationPropertyBinding": {
                    "Person": "Persons"
                }
            }
        }
    }
}
