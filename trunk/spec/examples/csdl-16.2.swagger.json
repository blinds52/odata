{
    "x-odata-version": "4.0",
    "x-schemas": {
        "ODataDemo": {
            "uri": "http://host/service/$metadata"
        },
        "Some.Vocabulary.V1": {
            "uri": "http://somewhere/Vocabulary/V1"
        },
        "Vocabulary1": {
            "aliasFor": "Some.Vocabulary.V1"
        },
        "External.Annotations": {
            "annotations": {
                "ODataDemo.Supplier": {
                    "@Vocabulary1.EMail": {
                        "@odata.null": {

                        }
                    },
                    "@Vocabulary1.AccountID": {
                        "@odata.path": "ID"
                    },
                    "@Vocabulary1.Title": "Supplier Info",
                    "@Vocabulary1.DisplayName": {
                        "@odata.apply": "odata.concat",
                        "parameterValues": [
                            {
                                "@odata.path": "Name"
                            },
                            " in ",
                            {
                                "@odata.path": "Address/CountryName"
                            }
                        ]
                    }
                },
                "ODataDemo.Product": {
                    "@Self.Tags": [
                        "MasterData"
                    ]
                }
            }
        }
    }
}
