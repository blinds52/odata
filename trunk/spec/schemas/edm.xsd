    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="TOptionallyQualifiedName">
    <xs:restriction base="xs:string">
      <!-- The below pattern represents the allowed identifiers in ECMA specification plus the '.' for namespace qualification  -->
