          "type":"string"
        },
        "Color":{
          "type":"string"
        },
        "TaxRate":{
          "type":"number"
        }
      },
      "navigationProperties":{
        "Category":{
          "type":"Model.Category",
          "nullable":false,
          "partner":"Products"
        },
        "Supplier":{
          "type":"Model.Supplier",
          "nullable":false,
          "partner":"Products"
        },
        "OrderItems":{
          "type":"Collection(Model.OrderItem)",
          "partner":"Product"
        },
        "Sales":{
          "type":"Collection(Model.Sales)",
          "partner":"Product"
        }
      }
    },
    "ODataDemo.Order":{
      "$ref":"#/definitions/Model.Order"
    },
    "Model.Order":{
      "typekind":"EntityType",
      "keys":{
        "ID":"ID"
      },
      "properties":{
