<?xml version="1.0" encoding="utf-8"?>
<edmx:Edmx xmlns:edmx="http://docs.oasis-open.org/odata/ns/edmx" Version="4.0">
  <edmx:Reference Url="http://tinyurl.com/Org-OData-Measures-V1" />
  <edmx:DataServices>
    <edm:Schema xmlns:edm="http://docs.oasis-open.org/odata/ns/edm" Namespace="ODataDemo">
      <edm:EntityType Name="Product" HasStream="true">
        <edm:Key>
          <edm:PropertyRef Name="ID" />
        </edm:Key>
        <edm:Property Name="ID" Type="Int32" Nullable="false" />
        <edm:Property Name="Name" Type="String" Nullable="true" />
        <edm:Property Name="Description" Type="String" Nullable="true" />
        <edm:Property Name="ReleaseDate" Type="Date" Nullable="false" />
        <edm:Property Name="DiscontinuedDate" Type="Date" Nullable="true" />
        <edm:Property Name="Rating" Type="Int32" Nullable="false" />
        <edm:Property Name="Price" Type="Decimal" Nullable="false">
          <edm:Annotation Term="Org.OData.Measures.V1.ISOCurrency" Path="Currency" />
        </edm:Property>
        <edm:Property Name="Currency" Type="Edm.String" Nullable="false" MaxLength="3" />
        <edm:NavigationProperty Name="Category" Type="ODataDemo.Category" Nullable="false"
          Partner="Products"
        >
          <edm:OnDelete Action="Cascade" />
        </edm:NavigationProperty>
        <edm:NavigationProperty Name="Supplier" Type="ODataDemo.Supplier" Partner="Products" />
      </edm:EntityType>
      <edm:EntityType Name="Category">
        <edm:Key>
          <edm:PropertyRef Name="ID" />
        </edm:Key>
        <edm:Property Name="ID" Type="Edm.Int32" Nullable="false" />
        <edm:Property Name="Name" Type="Edm.String" Nullable="true" />
        <edm:NavigationProperty Name="Products" Partner="Category" Type="Collection(ODataDemo.Product)" />
      </edm:EntityType>
      <edm:EntityType Name="Supplier">
        <edm:Key>
          <edm:PropertyRef Name="ID" />
        </edm:Key>
        <edm:Property Name="ID" Type="Edm.Int32" Nullable="false" />
        <edm:Property Name="Name" Type="Edm.String" Nullable="true" />
        <edm:Property Name="Address" Type="ODataDemo.Address" Nullable="false" />
        <edm:Property Name="Concurrency" Type="Edm.Int32" Nullable="false" />
        <edm:NavigationProperty Name="Products" Partner="Supplier" Type="Collection(ODataDemo.Product)" />
        <edm:NavigationProperty Name="Country" Type="ODataDemo.Country">
          <edm:ReferentialConstraint Property="Address/Country" ReferencedProperty="Name" />
        </edm:NavigationProperty>
      </edm:EntityType>
      <edm:EntityType Name="Country">
        <edm:Key>
          <edm:PropertyRef Name="Name" />
        </edm:Key>
        <edm:Property Name="Name" Type="String" />
      </edm:EntityType>
      <edm:ComplexType Name="Address">
        <edm:Property Name="Street" Type="Edm.String" Nullable="true" />
        <edm:Property Name="City" Type="Edm.String" Nullable="true" />
        <edm:Property Name="State" Type="Edm.String" Nullable="true" />
        <edm:Property Name="ZipCode" Type="Edm.String" Nullable="true" />
        <edm:Property Name="Country" Type="Edm.String" Nullable="true" />
      </edm:ComplexType>
      <edm:EntityContainer Name="DemoService" IsDefaultEntityContainer="true">
        <edm:EntitySet Name="Products" EntityType="ODataDemo.Product">
          <edm:NavigationPropertyBinding Path="Category" EntitySet="ODataDemo.DemoService.Products" />
          <edm:NavigationPropertyBinding Path="Supplier" EntitySet="ODataDemo.DemoService.Suppliers" />
        </edm:EntitySet>
        <edm:EntitySet Name="Categories" EntityType="ODataDemo.Category">
          <edm:NavigationPropertyBinding Path="Products" EntitySet="ODataDemo.DemoService.Products" />
        </edm:EntitySet>
        <edm:EntitySet Name="Suppliers" EntityType="ODataDemo.Supplier">
          <edm:NavigationPropertyBinding Path="Products" EntitySet="ODataDemo.DemoService.Products" />
        </edm:EntitySet>
        <edm:Entity Name="Contoso" Type="Self.Supplier" />
        <edm:EntitySet Name="Countries" EntityType="ODataDemo.Country" />
        <edm:FunctionImport Name="ProductsByRating" EntitySet="Products" Function="ODataDemo.ProductsByRating" />
      </edm:EntityContainer>
    </edm:Schema>
  </edmx:DataServices>
</edmx:Edmx>