            "string":"This is weird, given that we have a Country entity type"
          }
        },
        "ContactName":{
          "type":"string"
        },
        "ContactTitle":{
          "type":"string"
        },
        "Fax":{
          "type":"string"
        },
        "Address":{
          "odataType":"Model.Address"
        },
        "EmailAddresses":{
          "odataType":"Collection(Edm.String)"
        },
        "Phone":{
          "odataType":"Model.PhoneNumber"
        }
      },
      "navigationProperties":{
        "Orders":{
          "type":"Collection(Model.Order)",
          "partner":"Customer"
        },
        "Sales":{
          "type":"Collection(Model.Sales)",
          "partner":"Customer"
        }
      }
    },
    "ODataDemo.Supplier":{
