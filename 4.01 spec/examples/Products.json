{
    "$Version": "4.0",
    "$EntityContainer": "ProductService.Container",
    "$Reference": {
        "http://localhost/examples/People.xml": {
            "$Include": [
                {
                    "$Namespace": "PeopleService",
                    "$Alias": "PeopleService_"
                }
            ]
        }
    },
    "ProductService": {
        "$Kind": "Schema",
        "$Alias": "self",
        "$Annotations": {
            "self.Container": {
                "@Display.Description": "This is a sample OData service with vocabularies"
            },
            "self.Product": {
                "@Display.Description": "All Products available in the online store"
            },
            "self.Product/Name": {
                "@Display.DisplayName": "Product Name"
            }
        },
        "Product": {
            "$Kind": "EntityType",
            "$Key": [
                "ID"
            ],
            "ID": {
                "$Type": "Edm.Int32",
                "$Nullable": false
            },
            "Name": {

            },
            "Description": {

            },
            "ReleaseDate": {
                "$Type": "Edm.DateTimeOffset",
                "$Nullable": false
            },
            "DiscontinuedDate": {
                "$Type": "Edm.DateTimeOffset"
            },
            "Rating": {
                "$Type": "Edm.Int16",
                "$Nullable": false
            },
            "Price": {
                "$Type": "Edm.Double",
                "$Nullable": false
            },
            "Categories": {
                "$Kind": "NavigationProperty",
                "$IsCollection": true,
                "$Type": "self.Category",
                "$Partner": "Products"
            },
            "Supplier": {
                "$Kind": "NavigationProperty",
                "$Type": "PeopleService_.Supplier",
                "$Partner": "Products"
            },
            "ProductDetail": {
                "$Kind": "NavigationProperty",
                "$Type": "self.ProductDetail",
                "$Partner": "Product"
            }
        },
        "FeaturedProduct": {
            "$Kind": "EntityType",
            "$BaseType": "self.Product",
            "Advertisement": {
                "$Kind": "NavigationProperty",
                "$Type": "self.Advertisement",
                "$Partner": "FeaturedProduct"
            }
        },
        "ProductDetail": {
            "$Kind": "EntityType",
            "$Key": [
                "ProductID"
            ],
            "ProductID": {
                "$Type": "Edm.Int32",
                "$Nullable": false
            },
            "Details": {

            },
            "Product": {
                "$Kind": "NavigationProperty",
                "$Type": "self.Product",
                "$Partner": "ProductDetail"
            }
        },
        "Category": {
            "$Kind": "EntityType",
            "$OpenType": true,
            "$Key": [
                "ID"
            ],
            "ID": {
                "$Type": "Edm.Int32",
                "$Nullable": false
            },
            "Name": {

            },
            "Products": {
                "$Kind": "NavigationProperty",
                "$IsCollection": true,
                "$Type": "self.Product",
                "$Partner": "Categories"
            }
        },
        "Advertisement": {
            "$Kind": "EntityType",
            "$HasStream": true,
            "$Key": [
                "ID"
            ],
            "ID": {
                "$Type": "Edm.Guid",
                "$Nullable": false
            },
            "Name": {

            },
            "AirDate": {
                "$Type": "Edm.DateTimeOffset",
                "$Nullable": false
            },
            "FeaturedProduct": {
                "$Kind": "NavigationProperty",
                "$Type": "self.FeaturedProduct",
                "$Partner": "Advertisement"
            }
        },
        "Discount": [
            {
                "$Kind": "Action",
                "$IsBound": true,
                "$Parameter": [
                    {
                        "$Name": "product",
                        "$Type": "self.Product"
                    },
                    {
                        "$Name": "discountPercentage",
                        "$Type": "Edm.Int32",
                        "$Nullable": false
                    }
                ],
                "$ReturnType": {
                    "$Type": "Edm.Double",
                    "$Nullable": false
                }
            }
        ],
        "Container": {
            "$Kind": "EntityContainer",
            "Products": {
                "$Kind": "EntitySet",
                "$Type": "self.Product",
                "$NavigationPropertyBinding": {
                    "self.FeaturedProduct/Advertisement": "Advertisements",
                    "Categories": "Categories",
                    "Supplier": "PeopleService_.Container/Suppliers",
                    "ProductDetail": "ProductDetails"
                }
            },
            "ProductDetails": {
                "$Kind": "EntitySet",
                "$Type": "self.ProductDetail",
                "$NavigationPropertyBinding": {
                    "Product": "Products"
                }
            },
            "Categories": {
                "$Kind": "EntitySet",
                "$Type": "self.Category",
                "$NavigationPropertyBinding": {
                    "Products": "Products"
                }
            },
            "Advertisements": {
                "$Kind": "EntitySet",
                "$Type": "self.Advertisement",
                "$NavigationPropertyBinding": {
                    "FeaturedProduct": "Products"
                }
            }
        }
    }
}
