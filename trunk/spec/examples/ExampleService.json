    },
    "Model.Category":{
      "typekind":"EntityType",
      "keys":{
        "ID":"ID"
      },
      "properties":{
        "ID":{
          "type":"integer",
          "minimum":-2147483648,
          "maximum":2147483647
        },
        "Amount":{
          "type":"number"
        },
        "Name":{
          "type":"string"
        },
        "Thumbnail":{
          "type":"string"
        }
      },
      "navigationProperties":{
        "Products":{
          "type":"Collection(Model.Product)",
          "partner":"Category"
        }
      }
    },
    "ODataDemo.OrderItem":{
      "$ref":"#/definitions/Model.OrderItem"
    },
    "Model.OrderItem":{
      "typekind":"EntityType",
      "keys":{
