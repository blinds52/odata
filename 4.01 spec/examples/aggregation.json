{
    "$Version": "4.0",
    "$EntityContainer": "org.example.odata.salesservice.Default",
    "$Reference": {
        "http://docs.oasis-open.org/odata/odata-vocabularies/v4.0/vocabularies/Org.OData.Aggregation.V1.json": {
            "$Include": [
                {
                    "$Namespace": "Org.OData.Aggregation.V1.",
                    "$Alias": "Aggregation"
                }
            ]
        }
    },
    "org.example.odata.salesservice.": {
        "$kind": "Schema",
        "$Alias": "SalesModel",
        "$Annotations": {
            "org.example.odata.salesservice.Product": {
                "@Org.OData.Aggregation.V1.LeveledHierarchy#ProductHierarchy": {
                    "Levels": [
                        "Category/Name",
                        "Name"
                    ]
                }
            },
            "org.example.odata.salesservice.Time": {
                "@Org.OData.Aggregation.V1.LeveledHierarchy#TimeHierarchy": {
                    "Levels": [
                        "Year",
                        "Quarter",
                        "Month"
                    ]
                }
            },
            "org.example.odata.salesservice.SalesOrganization": {
                "@Org.OData.Aggregation.V1.RecursiveHierarchy#SalesOrgHierarchy": {
                    "NodeProperty": "ID",
                    "ParentNodeProperty": "Superordinate/ID"
                }
            }
        }
    },
    "org.example.odata.salesservice.Currency": {
        "$kind": "EntityType",
        "$Key": [
            "Code"
        ],
        "Code": {
            "$kind": "Property",
            "$Type": "Edm.String",
            "@Org.OData.Aggregation.V1.Groupable": true
        },
        "Name": {
            "$kind": "Property",
            "$Type": "Edm.String",
            "@Core.IsLanguageDependent": true
        }
    },
    "org.example.odata.salesservice.Sales": {
        "$kind": "EntityType",
        "$Key": [
            "ID"
        ],
        "ID": {
            "$kind": "Property",
            "$Type": "Edm.String",
            "$Nullable": false
        },
        "Amount": {
            "$kind": "Property",
            "$Type": "Edm.Decimal",
            "$Scale": "variable",
            "@Org.OData.Aggregation.V1.Aggregateable": true,
            "@Org.OData.Aggregation.V1.ContextDefiningProperties": [
                {
                    "$PropertyPath": "Currency/Code"
                }
            ]
        },
        "Customer": {
            "$kind": "NavigationProperty",
            "$Type": "org.example.odata.salesservice.Customer",
            "$Nullable": false
        },
        "Currency": {
            "$kind": "NavigationProperty",
            "$Type": "org.example.odata.salesservice.Currency",
            "$Nullable": false
        },
        "Product": {
            "$kind": "NavigationProperty",
            "$Type": "org.example.odata.salesservice.Product",
            "$Nullable": false
        },
        "SalesOrganization": {
            "$kind": "NavigationProperty",
            "$Type": "org.example.odata.salesservice.SalesOrganization",
            "$Nullable": false
        },
        "Time": {
            "$kind": "NavigationProperty",
            "$Type": "org.example.odata.salesservice.Time",
            "$Nullable": false
        },
        "@Org.OData.Aggregation.V1.CustomAggregate#Forecast": "Edm.Decimal",
        "@Org.OData.Aggregation.V1.CustomAggregate#Forecast@Org.OData.Aggregation.V1.ContextDefiningProperties": [
            {
                "$PropertyPath": "Currency/Code"
            }
        ]
    },
    "org.example.odata.salesservice.Default": {
        "$kind": "EntityContainer",
        "@Org.OData.Aggregation.V1.ApplySupported": {
            "PropertyRestrictions": true
        },
        "@Org.OData.Aggregation.V1.CustomAggregate#Budget": "Edm.Decimal",
        "@Org.OData.Aggregation.V1.CustomAggregate#Budget@Org.OData.Aggregation.V1.ContextDefiningProperties": [
            {
                "$PropertyPath": "Currencies/Code"
            }
        ],
        "Sales": {
            "$kind": "EntitySet",
            "$Type": "org.example.odata.salesservice.Sales"
        },
        "Currencies": {
            "$kind": "EntitySet",
            "$Type": "org.example.odata.salesservice.Currency"
        }
    }
}
